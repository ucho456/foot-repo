{
  "name": "foot-repo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts --port 3001",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "emu": "firebase emulators:start --import=./.emulator --export-on-exit",
    "deploy": "firebase deploy",
    "deploy:firestore:rules": "firebase deploy --only firestore:rules",
    "export:firestore:indexes": "firebase firestore:indexes > firestore.indexes.json",
    "deploy:storage:rules": "firebase deploy --only storage:rules",
    "deploy:functions": "firebase deploy --only functions",
    "functions:shell": "firebase functions:shell",
    "functions:config:set": "firebase functions:config:set",
    "functions:config:get": "firebase functions:config:get > ./functions/.runtimeconfig.json",
    "functions:tsc": "npm --prefix ./functions run build"
  },
  "dependencies": {
    "@mdi/js": "^6.9.96",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.31.0",
    "browser-image-compression": "^1.0.17",
    "core-js": "^3.15.1",
    "firebase": "^9.6.7",
    "nuxt": "^2.15.7",
    "vee-validate": "^3.4.14",
    "vuetify": "^2.5.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/firebase": "^8.2.2",
    "@nuxtjs/google-fonts": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0"
  }
}
